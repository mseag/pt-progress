{
  "name": "pt-progress",
  "version": "0.1.0",
  "description": "Utility to convert project progress from Excel file to Paratext",
  "main": "dist/index",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint . --ext .ts",
    "publish": "pkg dist/index.js --output pt-progress.exe",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -w & nodemon -q -w dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mseag/pt-progress.git"
  },
  "keywords": [
    "excel",
    "paratext",
    "json",
    "xml"
  ],
  "author": "SIL International",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mseag/pt-progress/issues"
  },
  "homepage": "https://github.com/mseag/pt-progress#readme",
  "dependencies": {
    "@types/vkbeautify": "^0.99.2",
    "@types/xml2json": "^0.11.3",
    "commander": "^5.1.0",
    "convert-excel-to-json": "^1.7.0",
    "path": "^0.12.7",
    "vkbeautify": "^0.99.3",
    "xml-formatter": "^1.2.1",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
